{
  "name": "express-api",
  "version": "1.0.0",
  "private": true,
  "description": "An Express-Sequelize template",
  "scripts": {
    "dev": "node ./api/api.js",
    "fixtures": "node ./api/fixtures.js",
    "diagram": "node ./api/diagram.js",
    "lint": "eslint ./api/. ./config/.",
    "start": "yarn run nodemon",
    "nodemon": "cross-env NODE_ENV=development nodemon --exec 'yarn run dev'",
    "prepush": "yarn test",
    "pretest": "yarn run lint",
    "production": "cross-env NODE_ENV=production node ./api/api.js"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "api/fixtures/**/*"
    ],
    "verbose": true,
    "watch": [
      "api/",
      "config/"
    ],
    "ext": "js,json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.53.1",
    "@aws-sdk/signature-v4-crt": "^3.53.0",
    "bcrypt": "^5.0.1",
    "bson": "^4.6.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "pluralize": "^8.0.0",
    "sequelize": "^6.17.0",
    "sequelize-fixtures": "^1.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "fs-extra": "^10.0.1",
    "nodemon": "^2.0.15"
  }
}
